
@{
    Layout = null;
}

@{
    ViewBag.Title = "Adivsor | Dashboard";
}


<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="icon" href="/favicon.ico" type="image/x-icon">

    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">

    <link href="/plugins/bootstrap/css/bootstrap.css" rel="stylesheet">
    <link href="/plugins/node-waves/waves.css" rel="stylesheet" />
    <link href="/plugins/animate-css/animate.css" rel="stylesheet" />
    <link href="/plugins/morrisjs/morris.css" rel="stylesheet" />
    <link href="/css/style.css" rel="stylesheet">
    <link href="/css/themes/all-themes.css" rel="stylesheet" />

</head>

<body class="theme-red">
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="preloader">
                <div class="spinner-layer pl-red">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
            <p>Please wait...</p>
        </div>
    </div>

    <script type="text/javascript" src="~/js/topbar.js"></script>

    <!-- #Top Bar -->

    <section>
        <!-- Left Sidebar -->
        <aside id="leftsidebar" class="sidebar">
            <!-- User Info -->
            <script type="text/javascript" src="~/js/userinfo.js"></script>

            <!-- #User Info -->
            <!-- Menu -->
            <div class="menu">
                @if (Convert.ToInt32(Session["roleteach"]) == 1)
                {
                    <script type="text/javascript" src="~/js/menuadmin.js"></script>
                }
                @if (Convert.ToInt32(Session["roleteach"]) == 2)
                {
                    <ul class="list">
                        <script type="text/javascript" src="~/js/menuadvisortop.js"></script>
                        @if (Convert.ToInt32(Session["sem"]) == 3 || Convert.ToInt32(Session["sem"]) == 4)
                        {
                            <script type="text/javascript" src="~/js/menuadvisorSEdefaulter.js"></script>
                        }
                        @if (Convert.ToInt32(Session["sem"]) == 5 || Convert.ToInt32(Session["sem"]) == 6)
                        {
                            <script type="text/javascript" src="~/js/menuadvisorTEdefaulter.js"></script>
                        }
                        @if (Convert.ToInt32(Session["sem"]) == 7 || Convert.ToInt32(Session["sem"]) == 8)
                        {
                            <script type="text/javascript" src="~/js/menuadvisorBEdefaulter.js"></script>
                        }

                        <script type="text/javascript" src="~/js/menuadvisorbottom.js"></script>

                    </ul>
                }
                @if (Convert.ToInt32(Session["roleteach"]) == 3)
                {
                    <script type="text/javascript" src="~/js/menuteacher.js"></script>
                }

            </div>
            <!-- #Menu -->
            <!-- Footer -->
            <script type="text/javascript" src="~/js/footer.js"></script>
            <!-- #Footer -->
        </aside>
        <!-- #END# Left Sidebar -->

    </section>



    <section class="content">
        <div class="container-fluid">
            <div class="block-header">
                <h2>DASHBOARD</h2>
            </div>


            <div class="row clearfix">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="card">
                        <div class="header">
                            <h2> Upload Student List </h2>
                        </div>

                        <div class="body">

                            @using (Html.BeginForm("uploadstudent", "uploadstudent", FormMethod.Post, new { enctype = "multipart/form-data" }))
                            {
                                <input type="file" name="studentfile" />
                                <input type="submit" value="OK" />

                            }
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </section>


    <script src="/plugins/jquery/jquery.min.js"></script>
    <script src="/plugins/bootstrap/js/bootstrap.js"></script>
    <script src="/plugins/bootstrap-select/js/bootstrap-select.js"></script>
    <script src="/plugins/jquery-slimscroll/jquery.slimscroll.js"></script>
    <script src="/plugins/node-waves/waves.js"></script>
    <script src="/plugins/jquery-countto/jquery.countTo.js"></script>
    <script src="/js/admin.js"></script>
    <script src="/js/pages/index.js"></script>
    <script src="/js/demo.js"></script>
</body>

</html>