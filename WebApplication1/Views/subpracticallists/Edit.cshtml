@model WebApplication1.Models.subpracticallist


@{
    Layout = null;
}
@{
    ViewBag.Title = "Edit Subject Details| Dashboard";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">
    <link href="/plugins/bootstrap/css/bootstrap.css" rel="stylesheet">
    <link href="/plugins/node-waves/waves.css" rel="stylesheet" />
    <link href="/plugins/animate-css/animate.css" rel="stylesheet" />
    <link href="/css/style.css" rel="stylesheet">
    <link href="/css/themes/all-themes.css" rel="stylesheet" />
    <link href="/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />

</head>
<body class="theme-red">
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")

    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="preloader">
                <div class="spinner-layer pl-red">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
            <p>Please wait...</p>
        </div>
    </div>

    <script type="text/javascript" src="~/js/topbar.js"></script>

    <!-- #Top Bar -->

    <section>
        <!-- Left Sidebar -->
        <aside id="leftsidebar" class="sidebar">
            <!-- User Info -->
            <script type="text/javascript" src="~/js/userinfo.js"></script>

            <!-- #User Info -->
            <!-- Menu -->
            <div class="menu">
                @if (Convert.ToInt32(Session["roleteach"]) == 1)
                {
                    <script type="text/javascript" src="~/js/menuadmin.js"></script>
                }
                @if (Convert.ToInt32(Session["roleteach"]) == 2)
                {
                    <ul class="list">
                        <script type="text/javascript" src="~/js/menuadvisortop.js"></script>
                        @if (Convert.ToInt32(Session["sem"]) == 3 || Convert.ToInt32(Session["sem"]) == 4)
                        {
                            <script type="text/javascript" src="~/js/menuadvisorSEdefaulter.js"></script>
                        }
                        @if (Convert.ToInt32(Session["sem"]) == 5 || Convert.ToInt32(Session["sem"]) == 6)
                        {
                            <script type="text/javascript" src="~/js/menuadvisorTEdefaulter.js"></script>
                        }
                        @if (Convert.ToInt32(Session["sem"]) == 7 || Convert.ToInt32(Session["sem"]) == 8)
                        {
                            <script type="text/javascript" src="~/js/menuadvisorBEdefaulter.js"></script>
                        }

                        <script type="text/javascript" src="~/js/menuadvisorbottom.js"></script>

                    </ul>
                }
                @if (Convert.ToInt32(Session["roleteach"]) == 3)
                {
                    <script type="text/javascript" src="~/js/menuteacher.js"></script>
                }

            </div>
            <!-- #Menu -->
            <!-- Footer -->
            <script type="text/javascript" src="~/js/footer.js"></script>
            <!-- #Footer -->
        </aside>
        <!-- #END# Left Sidebar -->

    </section>



    <section class="content">
        <div class="container-fluid">
            <div class="block-header">
                <h2>Edit Subject Details</h2>
            </div>

            <div class="card col-md-6 col-md-offset-3">
                <div class="body ">

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()
                        <form>
                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                            @Html.HiddenFor(model => model.id)
                            <div class="input-group">
                                <span class="input-group-addon">
                                    @*<i class="material-icons">person</i>*@
                                    @Html.LabelFor(model => model.name, htmlAttributes: new { @class = "control-label col-md-2" })
                                </span>
                                <div class="form-line">
                                    @Html.EditorFor(model => model.name, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.name, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="input-group">
                                <span class="input-group-addon">
                                    @*<i class="material-icons">email</i>*@
                                @Html.LabelFor(model => model.sem, htmlAttributes: new { @class = "control-label col-md-2" })
                                </span>
                           
                                @Html.DropDownListFor(model => model.sem, new SelectList(Model.selectpracsem, "sem", "sem"), "Select sem", new { @class = "form-control " })
                                @Html.ValidationMessageFor(model => model.sem, "", new { @class = "text-danger" })
                         
                            </div>
                            <div class="input-group">
                                <span class="input-group-addon">
                                    @*<i class="material-icons">person</i>*@
                                   @Html.LabelFor(model => model.year, htmlAttributes: new { @class = "control-label col-md-2" })
                                </span>
 
                                    @Html.DropDownListFor(model => model.year, new SelectList(Model.selectpracyear, "year", "year"), "Select year", new { @class = "form-control " })
                                    @Html.ValidationMessageFor(model => model.year, "", new { @class = "text-danger" })
                             
                            </div>
                            <div class="input-group">
                                <span class="input-group-addon">
                                    @*<i class="material-icons">person</i>*@
                                    @Html.LabelFor(model => model.batch, htmlAttributes: new { @class = "control-label col-md-2" })
                                </span>

                               @Html.EditorFor(model => model.batch, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.batch, "", new { @class = "text-danger" })

                            </div>

                            
                            <input type="submit" value="Save" class="btn btn-block btn-lg bg-pink waves-effect" />

                        </form>


                        <br />
                        <div>
                            @Html.ActionLink("Back to List", "Index")
                        </div>
                    }
                </div>
            </div>
        </div>
    </section>


    <script src="/plugins/jquery/jquery.min.js"></script>
    <script src="/plugins/bootstrap/js/bootstrap.js"></script>
    <script src="/plugins/bootstrap-select/js/bootstrap-select.js"></script>
    <script src="/plugins/jquery-slimscroll/jquery.slimscroll.js"></script>
    <script src="/plugins/node-waves/waves.js"></script>
    <script src="/js/admin.js"></script>
    <script src="/js/demo.js"></script>
    <script src="/js/pages/forms/basic-form-elements.js"></script>
    <script src="/plugins/bootstrap-select/js/bootstrap-select.js"></script>

</body>
</html>